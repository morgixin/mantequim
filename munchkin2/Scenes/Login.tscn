script/source = "extends Node

signal CreateUser(username, password)
signal LoginUser(username, password)

# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	#$\"../Node/RichTextLabel\".text=\"claudinha123\"
	pass # Replace with function body.

# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta: float) -> void:
	pass 


# ON HOVER

### menu btns hover interaction
func _on_exit_btn_mouse_entered() -> void:
	$HBoxContainer/ExitBtn/RichTextLabel.text = \"[wave][center]\"+$HBoxContainer/ExitBtn/RichTextLabel.text
	
func _on_exit_btn_mouse_exited() -> void:
	$HBoxContainer/ExitBtn/RichTextLabel.text = \"[center]Cancelar\"

func _on_login_btn_mouse_entered() -> void:
	$HBoxContainer/LoginBtn/RichTextLabel.text = \"[wave][center]\"+$HBoxContainer/LoginBtn/RichTextLabel.text

func _on_login_btn_mouse_exited() -> void:
	$HBoxContainer/LoginBtn/RichTextLabel.text = \"[center]Logar\"


# ON CLICK

# muda de tela
func _on_exit_btn_pressed() -> void:
	queue_free()
	get_tree().change_scene_to_file(\"res://Scenes/Menu.tscn\")

func _on_login_btn_pressed() -> void:
	LoginUser.emit($UsernameLineEdit.text, $PasswordLineEdit.text)


func loginUser(username, password) -> void:
	LoginUser.emit(username, password)
"

